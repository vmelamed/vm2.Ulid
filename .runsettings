<RunSettings>
    <DataCollectionRunSettings>
        <DataCollectors>
            <DataCollector friendlyName="XPlat Code Coverage">
                <Configuration>
                    <Format>cobertura</Format>
                    <OutputDirectory>.coverage</OutputDirectory>
                    <FileName>coverage.cobertura.xml</FileName>
                    <Exclude>
                        <Namespace>System.Text.RegularExpressions.Generated</Namespace>
                        <Attribute>GeneratedCodeAttribute</Attribute>
                    </Exclude>
                </Configuration>
            </DataCollector>
        </DataCollectors>
    </DataCollectionRunSettings>
</RunSettings>
