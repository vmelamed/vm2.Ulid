{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Help",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--help"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark List",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--list",
                "flat"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark tree",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--list",
                "tree"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark NewUlid",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--filter",
                "*NewUlid*",
                "--memory",
                "--artifacts",
                "./benchmarks/BenchmarkDotNet.Artifacts"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark ParseUlid",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--filter",
                "*ParseUlid*",
                "--memory",
                "--artifacts",
                "./benchmarks/BenchmarkDotNet.Artifacts"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark UlidToString",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--filter",
                "*UlidToString*",
                "--memory",
                "--artifacts",
                "./benchmarks/BenchmarkDotNet.Artifacts"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
        {
            "name": "Benchmark All",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "dotnet: build",
            "program": "${workspaceFolder}/benchmarks/UlidType.Benchmarks/bin/Debug/net9.0/UlidType.Benchmarks",
            "args": [
                "--filter",
                "*",
                "--memory",
                "--artifacts",
                "./benchmarks/BenchmarkDotNet.Artifacts"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole"
        },
    ]
}
